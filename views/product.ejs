                                            <th class="borde-celda" style="text-align: center">Ubicación</th>
                                            <th class="borde-celda" style="text-align: center">Contrato</th>
                                            <th class="borde-celda" style="text-align: center">Área</th>
                                            <th class="borde-celda" style="text-align: center">Comentario</th>
                                        </tr>
                                    </thead>
                                    <tbody class="w3-border" style="font-size:11px;">
                                        <tr class="cuerpo" w3-repeat="voucherss">
