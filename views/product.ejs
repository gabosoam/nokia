            function guardarProducto() {
                var validator = $("#formIngreso").kendoValidator().data("kendoValidator"),
                    status = $(".status");

                if (validator.validate()) {

                    var barcode = getFormData($("#formIngreso")).barcode_input;

                    if (barcode == 's/n' || barcode == 'S/N') {
                        var cantidad = prompt('Ingrese la cantidad', 1)

                        if (parseInt(cantidad)) {
                            if (cantidad > 0) {
                                $('#txtCantidad').val(cantidad);
                                var data = $('#formIngreso').serialize();

                                $.post("/product/create2", data, function (data) {
                                    if (data == 'El ingreso del producto se realizó correctamente') {
                                        alert('Producto guardado');
                                        $('#grid2').data('kendoGrid').dataSource.read();
                                        $('#grid2').data('kendoGrid').refresh();
                                        actualizarReporte();

                                        $('#formIngreso')[0].reset();
                                    } else {
                                        alert(data);
                                    }
                                });

                            } else {
                                alert('Ingrese una cantidad mayor a cero')
                            }
                        } else {
                            alert('Ingrese una cantidad correcta')
                        }

                    } else {
                        $('#txtCantidad').val(1);
                        var data = $('#formIngreso').serialize();

                        $.post("/product/create2", data, function (data) {
                            if (data == 'El ingreso del producto se realizó correctamente') {
                                $('#formIngreso')[0].reset();
                                $('#grid2').data('kendoGrid').dataSource.read();
                                $('#grid2').data('kendoGrid').refresh();
                                actualizarReporte();
                                alert('Producto guardado');

                            } else {
                                alert(data);
                            }
                        });

                    }






                } else {

                }

            }

            function getFormData($form) {
                var unindexed_array = $form.serializeArray();
                var indexed_array = {};

                $.map(unindexed_array, function (n, i) {
                    indexed_array[n['name']] = n['value'];
                });

                return indexed_array;
            }

        </script>


