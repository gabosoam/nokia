                                    <button id="closeBill" class="btn btn-default">
                                        <span class="fa fa-times-circle"></span> Cerrar ingreso</button>
                                </div>


                                <button id="btnPrint" type="button" class="btn btn-default" onclick="print()">
                                    <span class="fa fa-print"></span> Imprimir</button>

                                 


                                <p class="cuerpo">
                                    <strong>Acta: </strong>
                                    <%=data.codigo%>
                                </p>
                                <p class="cuerpo">
                                    <strong>Proveedor: </strong>
                                    <%=data.name%>
                                </p>
                                <p class="cuerpo">
                                    <strong>Empresa: </strong>
                                    <%=data.company%>
                                </p>
                                <p class="cuerpo">
                                    <strong>Fecha: </strong>
                                    <%=data.date%>
                                </p>


                                <form id="formIngreso">
                                    <hr>

                                    <input type="hidden" value="<%= bill %>" name="bill">
                                    <input type="hidden" value="1" id="txtCantidad" name="cant">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label for="txtCodigo">Código:</label>
                                            <input required id="txtCodigo" validationMessage="Este campo es obligatorio" name="code" style="width: 100%;" />
                                            <br>

                                        </div>
                                        <div class="col-md-3">

                                            <label for="txtSerie">Número de serie (S/N sin serie):</label>
                                            <input required id="txtSerie" validationMessage="Este campo es obligatorio" name="barcode" style="width: 100%;" />
                                            <br>



                                        </div>

                                        <div class="col-md-3">
                                            <label for="txtFdr">FDR:</label>
                                            <input class="form-control" name="fdr" placeholder="Ingresar FDR" id="txtFdr" name="fdr" style="width: 100%;" />
                                        </div>

                                        <div class="col-md-3">
                                            <label for="txtCodigo">CSO:</label>
                                            <input id="txtCso" placeholder="Ingresar CSO" name="cso" class="form-control" style="width: 100%;" />
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-3">
                                            <label for="txtCodigo">WBS:</label>
                                            <input class="form-control" placeholder="Ingresar WBS" name="wbs" id="txtWbs" style="width: 100%;" />
                                        </div>

                                        <div class="col-md-3">
                                            <label for="txtCodigo">Contrato:</label>
                                            <input required class="form-control" validationMessage="Este campo es obligatorio" placeholder="Ingresar contrato" name="contrato"
                                                id="txtContrato" style="width: 100%;" />
                                        </div>

                                        <div class="col-md-3">
                                            <label for="txtCodigo">Área:</label>
                                            <input class="form-control" placeholder="Ingresar área" name="area" id="txtAerea" style="width: 100%;" />
                                        </div>

                                        <div class="col-md-3">
                                            <label for="txtCodigo">Comentario:</label>
                                            <input class="form-control" placeholder="Ingresar comentario" name="comment" id="txtWbs" style="width: 100%;" />
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-3">
                                            <label for="txtSerie">Ubicación:</label>
                                            <input required validationMessage="Este campo es obligatorio" id="txtUbicacion" name="location" name="barcode" style="width: 100%;"
                                            />

                                        </div>

                                        <div class="col-md-3">
                                            <label for="txtSerie">Acción:</label>
                                            <br>

                                            <button onclick="guardarProducto()" class="btn btn-primary" type="button">Guardar</button>

                                        </div>

                                    </div>







                                    <hr>
                                </form>
                                <br>






                                <div class="row">

                                    <!-- <div id="actions">
