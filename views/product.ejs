        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Registrar modelo</h4>
                    </div>
                    <div class="modal-body">
                        <form id="formSaveModel">


                            <div class="form-group">
                                <label for="code">Código:</label>
                                <input autofocus type="text" class="form-control" validationMessage="Selecciona un producto" required id="codeModal" name="code"
                                    style="width: 100%">
                            </div>


                            <div class="form-group">
                                <label for="pwd">Nombre:</label>
                                <input type="text" class="form-control" validationMessage="Nombre" name="description" required id="description" style="width: 100%">
                            </div>



                            <div class="form-group">
                                <label for="pwd">Categoría:</label>
                                <input type="text" class="form-control" required name="category" id="category" style="width: 100%">
                            </div>





                            <div class="form-group">
                                <label for="pwd">Marca:</label>
                                <input type="text" class="form-control" required name="brand" id="brand" style="width: 100%">
                            </div>


                    </div>
                    <div class="modal-footer">
                        <button id="saveModel" type="button" class="btn btn-primary">Guardar</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                    </form>
                </div>
            </div>
        </div>

        <div class="modal inmodal fade" id="modalNuevo" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog modal-md">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            <span aria-hidden="true">&times;</span>
                            <span class="sr-only">Close</span>
                        </button>
                        <h4 class="modal-title">Agregar producto</h4>
                    </div>
                    <div class="modal-body">


                        <form role="form" id="form">
                            <div class="form-group">
                                <label>Número de serie</label>
                                <input pattern="[A-Za-z0-9]" id="barcode" maxlength="20" autofocus required type="text" name="barcode" placeholder="Ingresar número de serie"
                                    class="form-control">
                            </div>
                            <div class="form-group">
                                <label>FDR</label>
                                <input type="text" id="fdr" name="fdr" placeholder="Ingresar ficha de reparación" class="form-control">
                            </div>

                            <div class="form-group">
                                <label>CSO</label>
                                <input type="text" name="cso" placeholder="Ingresar CSO" class="form-control">
                            </div>



                            <div class="form-group">
                                <label>WBS</label>
                                <input type="text" name="wbs" placeholder="Ingresar WBS" class="form-control">
                            </div>

                            <div class="form-group">
                                <label>Seleccionar ubicación</label>
                                <select required class="form-control m-b" name="location">
                                    <option value="" disabled selected>Seleccionar una opción</option>
                                    <option>option 1</option>
                                    <option>option 2</option>
                                    <option>option 3</option>
                                    <option>option 4</option>
                                </select>
                            </div>

                            <div class="form-group">
                                <label>Comentario</label>
                                <textarea class="form-control" name="comentary" id="" cols="30" rows="5"></textarea>

                            </div>

                            <button type="submit">Subir</button>

                        </form>



                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-white" data-dismiss="modal">Cerrar</button>
                        <button type="button" class="btn btn-primary" onclick="guardarProducto()">Guardar</button>
                    </div>

                </div>
            </div>
