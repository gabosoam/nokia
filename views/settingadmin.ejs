        <div id="page-wrapper" class="gray-bg">



            <div class="wrapper wrapper-content">

                <div class="row">
                    <div class="col-md-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>
                                    <img src="../images/nokia.png" width="100px" alt="">
                                </h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>

                                </div>
                            </div>
                            <div class="ibox-content">
                                    

                            
                               <div class="ibox-content">
                                <div class="row">
                                    <div class="col-sm-6 b-r"><h3 class="m-t-none m-b">Cambiar contraseña</h3>
                            
                                        <form role="form" id="formPass">
                                            <input type="hidden" name="user" value="<%= user.id%>">
                                            <div class="form-group"><label>Contraseña anterior</label> <input required type="password" validationMessage="Este campo es obligatorio"  placeholder="Ingrese su contraseña actual" name="Anterior" class="form-control"></div>
                                            <div class="form-group"><label>Contraseña nueva</label> <input required type="password" validationMessage="Este campo es obligatorio" placeholder="Ingrese su contraseña nueva" name="Nueva" class="form-control"></div>
                                            <div class="form-group"><label>Repetir contraseña nueva</label> <input required type="password" validationMessage="Este campo es obligatorio" placeholder="Repita su contraseña nueva" name="Confirm" class="form-control"></div>
                                            <div>
                                                <button class="btn btn-sm btn-primary pull-right m-t-n-xs" type="button" onclick="cambiarContrasena()"><strong>Cambiar contraseña</strong></button>
                                             
                                            </div>
                                        </form>
                                    </div>
                                    <div class="col-sm-6">
                                   
                                    </div>
                                </div>
                            </div>

                            </div>
                        </div>
