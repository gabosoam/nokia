<div class="modal fade" id="modalReporte" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Historial</h4>
            </div>
            <div class="modal-body">

                <button onclick="print()" type="button" class="btn btn-default">
                    <span class="fa fa-print"></span> Imprimir</button>
                <div id="print" style="min-width: 210mm; font-family: 'Arial Narrow'">

                    <div class="w3-display-container" style="height:55px;">

                        <img id="auto" class="w3-display-middle" class="" src="../images/nokia.png" width="280px" height="55px" />

                    </div>

                    <div class="w3-display-container" style="height:25px;">
                        <h4 class="cuerpo w3-display-middle">Trazabilidad</h4>
                    </div>
                    <hr>

                    <div class="w3-display-container" style="height:100px;">

                        <div>
                            <p class="cuerpo">
                                <strong>Nombre: </strong>
                                <%= product.description %>
                            </p>
                            <p class="cuerpo">
                                <strong>Código: </strong>
                                <%= product.code %>
                            </p>
                            <p class="cuerpo">
                                <strong>Número de serie: </strong>
                                <%= product.barcode %>
                            </p>
                        </div>
                    </div>

                    

                    <hr>

                    <table id="Vouchers" class="w3-table w3-bordered" style="width: 100%">
                        <thead class="w3-border" style="font-size:13px;">
                            <tr class="cuerpo w3-border">
                                <th class="borde-celda" style="text-align: center">Tipo</th>
                                <th class="borde-celda" style="text-align: center">Código</th>
                                <th class="borde-celda" style="text-align: center">No. de serie</th>
                                <th class="borde-celda" class="borde-celda" style="text-align: center">Fecha</th>
                                <th class="borde-celda" style="text-align: center">FDR</th>
                                <th class="borde-celda" style="text-align: center">CSO</th>
                                <th class="borde-celda" style="text-align: center">WBS</th>
                                <th class="borde-celda" style="text-align: center">Contrato</th>
                                <th class="borde-celda" style="text-align: center">Área</th>
                                <th class="borde-celda" style="text-align: center">Ubicación</th>
                                <th class="borde-celda" style="text-align: center">Cant.</th>
                                <th class="borde-celda" style="text-align: center">Proveedor</th>
                                <th class="borde-celda" style="text-align: center">Empresa</th>
                                <th class="borde-celda" style="text-align: center">Cliente</th>
                                <th class="borde-celda" style="text-align: center">Empresa</th>
                                
                            </tr>
                        </thead>
                        <tbody class="w3-border" style="font-size:11px;">
                            <tr class="cuerpo" w3-repeat="voucherss">
                                <td class="borde-celda" style="text-align: center">{{type2}}</td>
                                <td class="borde-celda" style="text-align: center">{{code}}</td>
                                <td class="borde-celda" style="text-align: center">{{barcode}}</td>
                                <td class="borde-celda" style="text-align: center">{{date}}</td>
                                <td class="borde-celda" style="text-align: center">{{fdr}}</td>
                                <td class="borde-celda" style="text-align: center">{{cso}}</td>
                                <td class="borde-celda" style="text-align: center">{{wbs}}</td>
                                <td class="borde-celda" style="text-align: center">{{contrato}}</td>
                                <td class="borde-celda" style="text-align: center">{{area}}</td>
                                <td class="borde-celda" style="text-align: center">{{location}}</td>
                                <td class="borde-celda" style="text-align: center">{{cant}}</td>
                                <td class="borde-celda" style="text-align: center">{{name}}</td>
                                <td class="borde-celda" style="text-align: center">{{company}}</td>
                                <td class="borde-celda" style="text-align: center">{{client}}</td>
                                <td class="borde-celda" style="text-align: center">{{clientcompany}}</td>
                                
                            </tr>


                        </tbody>
                    </table>



               





